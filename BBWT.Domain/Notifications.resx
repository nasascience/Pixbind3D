  <data name="BaseTemplateLegend" xml:space="preserve">
    <value>&lt;div style='font-size: 10.0pt; font-family:Calibri,sans-serif;'&gt;
  &lt;div&gt;Legend:&lt;/div&gt;
  &lt;div&gt;
    &lt;table&gt;
      &lt;tr&gt;
        &lt;td&gt;$AppName&lt;/td&gt;
        &lt;td&gt;The name of web application&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td&gt;$DateTime&lt;/td&gt;
        &lt;td&gt;The current date &amp; time&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td&gt;$UserName&lt;/td&gt;
        &lt;td&gt;User name (login)&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td&gt;$UserEmail&lt;/td&gt;
        &lt;td&gt;User email address&lt;/td&gt;
      &lt;/tr&gt;
    &lt;/table&gt;
  &lt;/div&gt;
&lt;/div&gt;</value>
  </data>
  <data name="NewUserNotificationFrom" xml:space="preserve">
    <value>support@bbconsult.co.uk</value>
  </data>
  <data name="NewUserNotificationMessage" xml:space="preserve">
    <value>&lt;div style='font-size: 10.0pt; font-family:Calibri,sans-serif;'&gt;
  &lt;span&gt;Hello #$UserName#&lt;/span&gt;&lt;br /&gt;
  &lt;br /&gt;
  &lt;span&gt;We would like to welcome you to #$AppName#.&lt;/span&gt;&lt;br /&gt;
  &lt;span&gt;Please go to &lt;a href='#$UserLink#'&gt;your account page&lt;/a&gt; and enter the requested information.&lt;/span&gt;&lt;br /&gt;
  &lt;span&gt;We're happy to have you with us at #$AppName#.&lt;/span&gt;&lt;br /&gt;
  &lt;br /&gt;
  &lt;span&gt;Best regards, #$AppName# team.&lt;/span&gt;
&lt;/div&gt;</value>
  </data>
  <data name="NewUserNotificationNotes" xml:space="preserve">
    <value />
  </data>
  <data name="NewUserNotificationSubject" xml:space="preserve">
    <value>Welcome to #$AppName#</value>
  </data>
  <data name="ProblemNotificationFrom" xml:space="preserve">
    <value>&lt;"#$ProblemUserName#"&gt; #$ProblemUserEmail#</value>
  </data>
  <data name="ProblemNotificationMessage" xml:space="preserve">
    <value>&lt;div style='font-size: 10.0pt; font-family:Calibri,sans-serif;'&gt;
  &lt;span&gt; Report a Problem: &lt;/span&gt;&lt;span style='color: Red;'&gt; &amp;nbsp;#$DateTime# &lt;/span&gt;
  &lt;br/&gt;
  &lt;span style='color: Red;'&gt; User: &lt;/span&gt;&lt;span style='color: Red;'&gt; &amp;nbsp;#$ProblemUserName# &lt;/span&gt;
  &lt;br/&gt;
  &lt;span&gt; User Email: &lt;/span&gt;&lt;span&gt; &amp;nbsp;#$ProblemUserEmail# &lt;/span&gt;
  &lt;br/&gt;
  &lt;span style='color: Red;'&gt;Time of Problem: &lt;/span&gt;&lt;span style='color: Red;'&gt; &amp;nbsp;#$ProblemDateTime# &lt;/span&gt;
  &lt;br/&gt;
  &lt;br/&gt;
  &lt;span&gt; Problem Description: &lt;/span&gt;&lt;span&gt; #$ProblemDescription# &lt;/span&gt;
  &lt;br/&gt;
  &lt;span&gt; Problem Severity: &lt;/span&gt;&lt;span&gt; #$ProblemSeverity# &lt;/span&gt;
  &lt;br/&gt;
  &lt;br/&gt;
  &lt;span&gt; #$ProblemPreviousPage# &lt;/span&gt;
  &lt;br/&gt;
  &lt;span&gt; #$ProblemErrorMessages# &lt;/span&gt;
&lt;/div&gt;</value>
  </data>
  <data name="ProblemNotificationNotes" xml:space="preserve">
    <value />
  </data>
  <data name="ProblemNotificationSubject" xml:space="preserve">
    <value>Report a problem: #$DateTime#</value>
  </data>
  <data name="ResetPasswordByAdminNotificationMessage" xml:space="preserve">
    <value>&lt;div style='font-size: 10.0pt; font-family:Calibri,sans-serif;'&gt;&lt;span&gt;Please change your password.&lt;/span&gt;&lt;br /&gt;
  &lt;span&gt;To initiate the password reset process for your #$AppName# Account, click &lt;a href='#$UserLink#'&gt;here&lt;/a&gt;.&lt;/span&gt;&lt;br /&gt;
  &lt;br /&gt;
  &lt;span&gt;If clicking the link above doesn't work, please copy and paste the URL in a new browser window instead.&lt;/span&gt;&lt;br /&gt;  
  &lt;span&gt;Thank you for using #$AppName#.&lt;/span&gt;&lt;br /&gt;
  &lt;br /&gt;
  &lt;span&gt;This is a post-only mailing.  Replies to this message are not monitored or answered.&lt;/span&gt;
&lt;/div&gt;</value>
  </data>
  <data name="ResetPasswordNotificationMessage" xml:space="preserve">
    <value>&lt;div style='font-size: 10.0pt; font-family:Calibri,sans-serif;'&gt;
  &lt;span&gt;To initiate the password reset process for your #$AppName# Account, click &lt;a href='#$UserLink#'&gt;here&lt;/a&gt;.&lt;/span&gt;&lt;br /&gt;
  &lt;br /&gt;
  &lt;span&gt;If clicking the link above doesn't work, please copy and paste the URL in a new browser window instead.&lt;/span&gt;&lt;br /&gt;
  &lt;span&gt;If you've received this mail in error, it's likely that another user entered your email address by mistake while trying to reset a password.
                         If you didn't initiate the request, you don't need to take any further action and can safely disregard this email.&lt;/span&gt;&lt;br /&gt;
  &lt;span&gt;Thank you for using #$AppName#.&lt;/span&gt;&lt;br /&gt;
  &lt;br /&gt;
  &lt;span&gt;This is a post-only mailing.  Replies to this message are not monitored or answered.&lt;/span&gt;
&lt;/div&gt;</value>
  </data>
  <data name="ResetPasswordNotificationSubject" xml:space="preserve">
    <value>Reset password for #$AppName#</value>
